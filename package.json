  "main": "build/src/index.js",
  "bin": {
    "webtreemap": "build/src/cli.js"
  },
  "dependencies": {
    "commander": "^2.11.0",
    "open": "^7.2.1",
    "tmp": "^0.2.1"
  },
  "devDependencies": {
